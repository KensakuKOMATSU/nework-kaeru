  "description": "unofficial nework app using electron",
  "main": "src/index.js",
  "dependencies": {},
  "devDependencies": {
    "electron": "^10.1.1",
    "electron-packager": "^15.1.0"
  },
  "scripts": {
    "start": "NODE_ENV=development electron ./src",
    "build-macOS": "electron-packager ./src nework-kaeru --platform=darwin --arch=x64 --overwrite --icon=./src/assets/nework-kaeru.icns",
    "build-windows": "electron-packager ./src nework-kaeru --platform=win32 --arch=x64 --overwrite --icon=./src/assets/icon_256x256.ico",
